{
  "desc": "Auto generated by fswatch [dragon]",
  "triggers": [
    {
      "name": "",
      "pattens": [
        "**/*.go"
      ],
      "env": {
        "DEBUG": "1"
      },
      "cmd": "go fmt dragon... && go build -o ./release/dragon && ./release/dragon",
      "shell": true,
      "delay": "100ms",
      "stop_timeout": "500ms",
      "signal": "KILL",
      "kill_signal": ""
    }
  ],
  "watch_paths": [
    "."
  ],
  "watch_depth": 10
}